  "version": "0.2.0",
  "configurations": [
    {
      "type": "java",
      "name": "Launch Current File",
      "request": "launch",
      "mainClass": "${file}"
    },
    {
      "type": "java",
      "name": "Launch CustomAgent",
      "request": "launch",
      "mainClass": "com.sas.ci360.agent.CustomAgent",
      "projectName": "ci360-debug-agent",
      "vmArgs": [
        "-Dlogback.configurationFile=config/logback.xml",
        "-DconfigFile=config/agent.config"
      ]
    }
  ]
}
